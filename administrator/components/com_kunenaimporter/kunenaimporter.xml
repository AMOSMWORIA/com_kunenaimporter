<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install>
<install type="component" version="1.5.19" method="upgrade">
	<name>KunenaImporter</name>
	<version>1.6.0-RC2</version>
	<creationDate>@kunenaversiondate@</creationDate>
	<build>@kunenaversionbuild@</build>
	<author>Kunena Team</author>
	<authorEmail>Kunena@Kunena.com</authorEmail>
	<authorUrl>http://www.Kunena.com</authorUrl>
	<copyright>www.Kunena.com</copyright>
	<license>GNU/GPL</license>
	<description>Imports external forum data (phpBB3, SMF2) into Kunena Forum.</description>

	<installfile>install.kunenaimporter.php</installfile>
	<uninstallfile>uninstall.kunenaimporter.php</uninstallfile>

	<administration>
		<languages folder="languages">
			<language tag="en-GB">en-GB.com_kunenaimporter.ini</language>
		</languages>
		<menu view="default">Kunena Forum Importer</menu>
		<files folder="admin">
			<filename>config.xml</filename>
			<filename>controller.php</filename>
			<filename>index.html</filename>
			<filename>admin.kunenaimporter.php</filename>
			<folder>models</folder>
			<folder>views</folder>
			<folder>assets</folder>
		</files>
		<params/>
	</administration>
</install>